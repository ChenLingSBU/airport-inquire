<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>airportinquire</display-name>
  <servlet>
    <servlet-name>searchAirportWithRadiusServlet</servlet-name>
    <servlet-class>servlets.searchAirportWithRadiusServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>airportInfoDetailServlet</servlet-name>
  	<servlet-class>servlets.airportInfoDetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>searchAirportWithRadiusServlet</servlet-name>
    <url-pattern>/searchAirportWithRadiusServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>airportInfoDetailServlet</servlet-name>
  	<url-pattern>/airportInfoDetailServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>